<section class="singleTeam shadow-lg">
    <Container>
        <Row>
            <Col lg="10" class="m-auto">
            <div class="SingleTeamSection">
                <div class="teamRight">
                    <img class="card-img" src="/images/devs/<%= devs.photo %> " alt="" />
                    
                
                    <div class="team_info">
                        <h1><%= devs.name %> </h1>
                        <h4><%= devs.skill %> Developer</h4>
                        <div class="button">
                            <a href="tel:<%= devs.cell %> " class="btn btn-outline-primary mx-3"> Call Now</a>
                            <a href="mailto:<%= devs.email %> " class="btn btn-primary"> Message</a>
                        </div>
                        <hr />
                        <div class="social">
                            <ul>
                                <li>
                                    <a href="#">
                                        <i class="fa-brands fa-facebook"></i>
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <i class="fa-brands fa-twitter"></i>

                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <i class="fa-brands fa-linkedin"></i>

                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <i class="fa-brands fa-pinterest"></i>

                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <i class="fa-brands fa-instagram"></i>

                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="teamleft">
                    <h1> Personal Information</h1>
                    <hr />
                    <Table class="table">
                        <thead>
                            <tr>
                                <th><span><i class="fa-regular fa-address-card"></i></span> ID :</th>
                                <th><%= devs.id %></th>
                            </tr>
                            <tr>
                                <th><span><i class="fa-solid fa-user-tie"></i></span> Name :</th>
                                <th><%= devs.name %> </th>
                            </tr>
                            <tr>
                                <th><span><i class="fa-solid fa-mars-double"></i></span> Gender :</th>
                                <th><%= devs.gender %> </th>
                            </tr>
                            <tr>
                                <th><span><i class="fa-regular fa-envelope"></i></span> E-mail :</th>
                                <th><%= devs.email %> </th>
                            </tr>
                            <tr>
                                <th><span><i class="fa-solid fa-location-dot"></i></span> Location :</th>
                                <th><%= devs.location %> </th>
                            </tr>
                            <tr>
                                <th><span><i class="fa-regular fa-lightbulb"></i></span> Skill :</th>
                                <th><%= devs.skill %> </th>
                            </tr>
                            <tr>
                                <th><span><i class="fa-solid fa-phone"></i></span> Phone :</th>
                                <th><%= devs.cell %> </th>
                            </tr>
                        </thead>

                    </Table>


                    <div class="button">
                        <a href="/edit/<%= devs.id %>" class="btn btn-success mx-2">Edit Data</a>
                        <a href="/" class="btn btn-primary">Back To Home</a>
                    </div>
                </div>
            </div>
            </Col>
        </Row>
    </Container>
</section>