<div class="devs_data my-5">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-11">

                <a href="/create" class="btn btn-success">Add Developers</a>
                <br><br>
                <div class="card">
                    <div class="card-body">
                        <h1>All Developers</h1><hr>
                        <table class=" table table-striped devs_table">
                            <thead>
                                <tr>
                                    <th>Sl</th>
                                    <th>Name</th>
                                    <th>Gender</th>
                                    <th>Skill</th>
                                    <th>Location</th>
                                    <th>Email</th>
                                    <th>Cell</th>
                                    <th>Photo</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>

                                <% devs.reverse().forEach((data, index) => { %>

                                    <tr class="align-middle">
                                        <td><%= index + 1 %> </td>
                                        <td><%= data.name %></td>
                                        <td><%= data.gender %></td>
                                        <td><%= data.skill %></td>
                                        <td><%= data.location %></td>
                                        <td><%= data.email %></td>
                                        <td><%= data.cell %></td>
                                        <td><img style="width: 40px; height : 40px; object-fit: cover;" src="images/devs/<%= data.photo %>" alt=""></td>
                                        <td>
                                            <a href="/<%= data.id %>" class="btn btn-sm btn-primary"><i class="fa-regular fa-eye"></i></a>
    
                                            <a  href="/edit/<%= data.id %> " class="btn btn-sm btn-warning "><i class="fa-regular fa-pen-to-square"></i></a>
    
                                            <a href="/remove/<%= data.id %>" class="btn btn-sm btn-danger remove_devs"><i class="fa-solid fa-trash-can"></i></a>
                                        </td>
                                    </tr>
                                <% }) %>
                                


                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>